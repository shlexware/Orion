local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))() local Window = OrionLib:MakeWindow({Name = "لوحه محمد", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"}) --[[ Name = <string> - The name of the UI. HidePremium = <bool> - Whether or not the user details shows Premium status or not. SaveConfig = <bool> - Toggles the config saving in the UI. ConfigFolder = <string> - The name of the folder where the configs are saved. IntroEnabled = <bool> - Whether or not to show the intro animation. IntroText = <string> - Text to show in the intro animation. IntroIcon = <string> - URL to the image you want to use in the intro animation. Icon = <string> - URL to the image you want displayed on the window. CloseCallback = <function> - Function to execute when the window is closed. ]] local Tab = Window:MakeTab({ 	Name = "ادخل في سيرفر ديسكورد", 	Icon = "rbxassetid://4483345998", 	PremiumOnly = false }) --[[ Name = <string> - The name of the tab. Icon = <string> - The icon of the tab. PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only. ]] local Section = Tab:AddSection({ 	Name = "ادخل " }) --[[ Name = <string> - The name of the section. ]] OrionLib:MakeNotification({ 	Name = "اهلا بيك في لوحه محمد", 	Content = "Notification content... what will it say??", 	Image = "rbxassetid://4483345998", 	Time = 5 }) --[[ Title = <string> - The title of the notification. Content = <string> - The content of the notification. Image = <string> - The icon of the notification. Time = <number> - The duration of the notfication. ]] Tab:AddTextbox({ 	Name = "discord.gg", 	Default = "https://discord.gg/Z3X724gmuB", 	TextDisappear = true, 	Callback = function(Value) 		print(Value) 	end	 }) --[[ Name = <string> - The name of the textbox. Default = <string> - The default value of the textbox. TextDisappear = <bool> - Makes the text disappear in the textbox after losing focus. Callback = <function> - The function of the textbox. ]] local Tab = Window:MakeTab({ 	Name = "سكربتات لكل مابات", 	Icon = "rbxassetid://4483345998", 	PremiumOnly = false }) --[[ Name = <string> - The name of the tab. Icon = <string> - The icon of the tab. PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only. ]] Tab:AddButton({ 	Name = "سكربت تخفيف الاق", 	Callback = function() 		loadstring(game:HttpGet("https://pastebin.com/raw/eHEfAR8z", true))() 	end }) --[[ Name = <string> - The name of the button. Callback = <function> - The function of the button. ]] Tab:AddButton({ 	Name = "سكربت نسخ سريع لمابات الادمن فقط", 	Callback = function() 		loadstring(game:HttpGet("https://pastebin.com/raw/67zRS6vS", true))() 	end }) --[[ Name = <string> - The name of the button. Callback = <function> - The function of the button. ]] local Tab = Window:MakeTab({ 	Name = "سكربتات ماب بيوت", 	Icon = "rbxassetid://4483345998", 	PremiumOnly = false }) --[[ Name = <string> - The name of the tab. Icon = <string> - The icon of the tab. PremiumOnly = <bool> - Makes the tab accessible to Sirus Premium users only. ]] Tab:AddButton({ 	Name = "سكربت Redz", 	Callback = function() 		loadstring(game:HttpGet("https://raw.githubusercontent.com/REDzHUB/REDzHUB/main/REDzHUB"))() 	end }) --[[ Name = <string> - The name of the button. Callback = <function> - The function of the button. ]] Tab:AddButton({ 	Name = "سكربت icehub", 	Callback = function() 		loadstring(game:HttpGet("https://raw.githubusercontent.com/anyahubs/FE/main/ICEHUB_ARABIC"))() 	end }) --[[ Name = <string> - The name of the button. Callback = <function> - The function of the button. ]]
